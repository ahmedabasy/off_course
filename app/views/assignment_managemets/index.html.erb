<p id="notice"><%= notice %></p>

<h1>Listing Assignment Managemets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Deadline</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assignment_managemets.each do |assignment_managemet| %>
      <tr>
        <td><%= assignment_managemet.name %></td>
        <td><%= assignment_managemet.deadline %></td>
        <td><%= assignment_managemet.score %></td>
        <td><%= link_to 'Show', assignment_managemet %></td>
        <td><%= link_to 'Edit', edit_assignment_managemet_path(assignment_managemet) %></td>
        <td><%= link_to 'Destroy', assignment_managemet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assignment managemet', new_assignment_managemet_path %>
